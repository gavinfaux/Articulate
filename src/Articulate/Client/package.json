{
  "name": "articulate",
  "private": true,
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "watch": "npx chokidar \"src/**/*\" -c \"npm run build:copy\"",
    "build": "tsc && vite build && node scripts/copy-articulate-output.cjs",
    "build:copy": "tsc && vite build && node scripts/copy-articulate-output.cjs --copy",
    "generate-client": "node scripts/generate-openapi.js https://localhost:44366/umbraco/swagger/articulate-api/swagger.json",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.13.0",
    "monaco-editor": "^0.52.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@hey-api/openapi-ts": "^0.72.1",
    "@rollup/rollup-win32-x64-msvc": "^4.43.0",
    "@umbraco-cms/backoffice": "^15.4.3",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-local-rules": "3.0.2",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}

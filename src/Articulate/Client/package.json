{
  "name": "articulate",
  "private": true,
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "watch": "npx chokidar \"src/**/*\" -c \"npm run build\"",
    "build": "tsc && vite build && node scripts/copy-articulate-output.cjs",
    "build:copy": "tsc && vite build && node scripts/copy-articulate-output.cjs --copy",
    "generate-client": "node scripts/generate-openapi.js https://localhost:44366/umbraco/swagger/management/swagger.json?urls.primaryName=Umbraco+Management+API",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx",
    "typecheck": "npx tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.28.0",
    "@hey-api/client-fetch": "^0.11.0",
    "@hey-api/openapi-ts": "^0.69.2",
    "@rollup/rollup-win32-x64-msvc": "^4.41.1",
    "@umbraco-cms/backoffice": "^15.4.1",
    "chalk": "^5.4.1",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-local-rules": "3.0.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-wc": "3.0.1",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
